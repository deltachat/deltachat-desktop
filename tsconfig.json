{
  "compilerOptions": {
    "outDir": "./tsc-dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "rootDir": "src",
    "allowJs": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules", "**/node_modules/*",
    "test",
    "bin",
    "static",
    "src/renderer",
    "webpack.config.js",
    "index.js",
    "localize",
    "tsc-dist"
  ]
}